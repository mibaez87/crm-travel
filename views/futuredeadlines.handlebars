<div class="ui grid">
    <!--top of the page headers-->
    <div class="two wide column">
        <h2 class="ui block header" id="employeename">
            Jane Employee
        </h2>
    </div>
    <div class="eight wide column">
        <h1 id="deadlinesflightsdash">Deadlines and Flights</h1>
        <div id="maindeadlines">
            <h3 id="deadlinesheader">Deadlines</h3>
            <div class="ui styled fluid accordion" id="maindeadlinebody">
                <div class="active title" id="todaysdeadlines">
                    <i class="dropdown icon"></i>
                    Today
                </div>
                <div class="active content">
                    <table style="width:100%">
                    <tr>
                        <th>Client ID</th>
                        <th>Deposit Due</th>
                        <th>Cancellation Deadline</th>
                        <th>Final Payment</th>
                    </tr>
                    {{#each due_today}}
                    <tr>
                        <td>{{client_id}}</td>
                        <td>{{deposit_due}}</td>
                        <td>{{cancellation_deadline}}</td>
                        <td>{{final_payment}}</td>
                    </tr>
                    {{/each}}
                </table>
                </div>
                <div class="title" id="2weekdeadlines">
                    <i class="dropdown icon"></i>
                    Upcoming Two Weeks
                </div>
                <div class="content">
                    <table style="width:100%">
                    <tr>
                        <th>Client ID</th>
                        <th>Deposit Due</th>
                        <th>Cancellation Deadline</th>
                        <th>Final Payment</th>
                    </tr>
                    {{!-- {{#each due_today}}
