                        <td>{{cancellation_deadline}}</td>
                        <td>{{final_payment}}</td>
                    </tr>
                    {{/each}}
                </table>
                </div>
            </div>
        </div>
        <div id="flightslist">
            <h3 id="flightsheader">Flights</h3>
            <div class="ui styled fluid accordion" id="mainflightsbody">
                <div class="active title" id="todaysflights">
                    <i class="dropdown icon"></i>
                    Today
                </div>
                <div class="active content">
                    <table style="width:100%">
                    <tr>
                        <th>ID</th>
                        <th>Departure Date</th>
                        <th>Flight No.</th>
                        <th>Departing Airport</th>
                        <th>Departure Time</th>
                        <th>Arrival Date</th>
                        <th>Flight No.</th>
                        <th>Arriving Airport</th>
                        <th>Arrival Time</th>
                    </tr>
                    {{#each fly_today}}
                    <tr>
                        <td>{{client_id}}</td>
                        <td>{{dep_date}}</td>
                        <td>{{dep_flight}}</td>
                        <td>{{dep_airport}}</td>
                        <td>{{dep_time}}</td>
                        <td>{{arr_date}}</td>
                        <td>{{arr_flight}}</td>
                        <td>{{arr_airport}}</td>
                        <td>{{arr_time}}</td>
                    </tr>
                    {{/each}}
                </table>
                </div>
                <div class="title" id="1weekflights">
                    <i class="dropdown icon"></i>
                    Upcoming Week
