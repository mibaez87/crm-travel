                    <table style="width:100%">
                    <tr>
                        <th>Client ID</th>
                        <th>Deposit Due</th>
                        <th>Cancellation Deadline</th>
                        <th>Final Payment</th>
                    </tr>
                    {{#each next_thirty}}
